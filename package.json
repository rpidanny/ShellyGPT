{
  "name": "@rpidanny/shelly",
  "version": "1.0.0",
  "description": "A command-line tool that ingests data and generates instant answers using ChatGPT, giving you the Sheldon Cooper you never had at your fingertips.",
  "author": "rpidanny @rpidanny",
  "type": "module",
  "bin": {
    "shelly": "./bin/run.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/rpidanny/shelly",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "rpidanny/shelly",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:test": "NODE_OPTIONS=--experimental-vm-modules jest --watch ./test/commands/ingest.spec.ts",
    "prepack": "npm run build && oclif manifest && npm run version",
    "pack:macos": "oclif pack macos",
    "pack:win": "oclif pack win",
    "pack:deb": "oclif pack deb",
    "pack:tarballs": "oclif pack tarballs",
    "postpack": "shx rm -f oclif.manifest.json",
    "version": "oclif readme --multi",
    "start": "node ./bin/run.js",
    "prerelease": "npm run version",
    "release": "semantic-release"
  },
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "@oclif/core": "^2",
    "@oclif/plugin-autocomplete": "^2.1.9",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.23",
    "@oclif/plugin-plugins": "^2.4.6",
    "@pinecone-database/pinecone": "^0.0.14",
    "@zilliz/milvus2-sdk-node": "^2.2.7",
    "chalk": "^4.1.2",
    "epub2": "^3.0.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5",
    "langchain": "^0.0.66",
    "marked": "^4.3.0",
    "marked-terminal": "^5.1.1",
    "pdf-parser": "^1.0.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@oclif/config": "^1.18.8",
    "@oclif/test": "^2.3.16",
    "@rpidanny/eslint-config-typescript": "^1.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^10.0.3",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@tsconfig/recommended": "^1.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.1",
    "@types/marked": "^4.0.8",
    "@types/marked-terminal": "^3.1.3",
    "@types/node": "^16.18.24",
    "chai": "^4.3.7",
    "commitlint": "^17.6.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "esm": "^3.2.25",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.2",
    "mocha": "^10.2.0",
    "nock": "^13.3.1",
    "oclif": "^3",
    "semantic-release": "^21.0.1",
    "shx": "^0.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "shelly",
    "dirname": "shelly",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {},
    "hooks": {
      "init": "./dist/hooks/init/config"
    },
    "macos": {
      "identifier": "com.rpidanny.shelly"
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/rpidanny/shelly/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
